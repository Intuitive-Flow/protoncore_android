<resources>
  <!-- Grace period - info -->
  <string name="account_recovery_grace_period_info_title">密碼重設已依需求送出</string>
  <plurals name="account_recovery_grace_period_info_subtitle">
    <item quantity="other"><b>%1$s</b> 的密碼重設需求已寄發。您可以在接下來 %2$d 小時內變更密碼。\n\n 若您並未提出此需求，請即刻進行取消。</item>
  </plurals>
  <!-- Grace period - cancellation form -->
  <string name="account_recovery_cancel_title">取消密碼重設？</string>
  <string name="account_recovery_cancel_subtitle">請輸入您的密碼以便取消需求。</string>
  <string name="account_recovery_cancel_password_label">目前使用的密碼</string>
  <string name="account_recovery_cancel_password_placeholder">密碼</string>
  <string name="account_recovery_cancel_now">取消密碼重設</string>
  <string name="account_recovery_cancel_back">返回</string>
  <!-- Account recovery cancelled -->
  <string name="account_recovery_cancelled_title">已取消密碼重設</string>
  <string name="account_recovery_cancelled_subtitle">若您未曾要求更改密碼，那麼您的帳號可能存在風險。\n\n請即刻變更您的密碼才得以安全保護您的帳號。</string>
  <!-- Password change period -->
  <string name="account_recovery_password_started_title">現在可進行密碼重設</string>
  <string name="account_recovery_password_started_subtitle">為了重設密碼，請回到您原先做出密碼重設要求的裝置、應用程式或瀏覽器。\n\n您必須在 %s 之前完成。\n\n若您並未要求重設密碼，請立刻取消要求。</string>
  <string name="account_recovery_password_started_self_initiated_subtitle">現在您可以重設密碼。\n\n您需要在 %s 之前執行此操作。\n\n若您沒有要求重設密碼，請立刻取消要求。</string>
  <!-- Recovery window ended -->
  <string name="account_recovery_window_ended_title">密碼重設要求已過期</string>
  <string name="account_recovery_window_ended_subtitle">您將需要為 %s 重設密碼。</string>
  <!-- Other -->
  <string name="account_recovery_cancel">取消要求</string>
  <string name="account_recovery_dismiss">瞭解</string>
  <string name="account_recovery_reset">重設密碼</string>
  <!-- Account Recovery Info -->
  <string name="account_recovery_info_grace_title">密碼重設已被送出</string>
  <string name="account_recovery_info_grace_subtitle">您可以在 %1s 內重設密碼。</string>
  <string name="account_recovery_info_grace_description">為了確保確實是您在嘗試重設密碼，我們會等待 72 小時才核准請求。\n\n若您沒有要求重設密碼，請立即取消此要求。</string>
  <string name="account_recovery_info_cancelled_title">已取消密碼重設</string>
  <string name="account_recovery_info_cancelled_subtitle">密碼重設要求於 %1s 發出。</string>
  <string name="account_recovery_info_cancelled_description">若您不認得此要求，則可能代表其他人可以存取您的 Proton 帳號。\n\n立刻保護您的帳號。</string>
  <string name="account_recovery_info_insecure_title">可以重設密碼</string>
  <string name="account_recovery_info_insecure_subtitle">現在，您可以重設密碼了。可以在 %1s 前重設密碼。</string>
  <string name="account_recovery_info_insecure_description">並未提出此要求？\n\n若您並未要求重設密碼，請立刻取消要求。</string>
  <!-- Password Reset Dialog -->
  <string name="account_recovery_reset_dialog_title">重設密碼?</string>
  <string name="account_recovery_reset_dialog_text">"出於安全原因，您必須等待 72 小時才能變更 %1s 的密碼。\n\n如果您設定了備援方法，請嘗試帳號還原。這將讓您可以立刻變更密碼。"</string>
  <string name="account_recovery_reset_dialog_action_request_reset">需求重設密碼</string>
  <string name="account_recovery_reset_dialog_action_use_recovery">使用備援方法</string>
  <string name="account_recovery_reset_dialog_action_use_recovery_hint">在網頁瀏覽器中變更密碼，不需要等待安全期結束。</string>
  <string name="account_recovery_reset_dialog_action_request_reset_success">密碼重設已被送出</string>
</resources>
