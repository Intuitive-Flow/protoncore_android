<resources>
  <!-- Grace period - info -->
  <string name="account_recovery_grace_period_info_title">Zatražen zahtjev za ponovno postavljanje lozinke</string>
  <plurals name="account_recovery_grace_period_info_subtitle">
    <item quantity="one">A password reset was requested for <b>%1$s</b>. You can change your password in %2$d hour.\n\nIf you didn’t make this request, cancel it now.</item>
    <item quantity="few">A password reset was requested for <b>%1$s</b>. You can change your password in %2$d hours.\n\nIf you didn’t make this request, cancel it now.</item>
    <item quantity="other">A password reset was requested for <b>%1$s</b>. You can change your password in %2$d hours.\n\nIf you didn’t make this request, cancel it now.</item>
  </plurals>
  <!-- Grace period - cancellation form -->
  <string name="account_recovery_cancel_title">Poništiti ponovno postavljanje lozinke?</string>
  <string name="account_recovery_cancel_subtitle">Unesite vašu lozinku za otkazivanje.</string>
  <string name="account_recovery_cancel_password_label">Trenutna lozinka</string>
  <string name="account_recovery_cancel_password_placeholder">Lozinka</string>
  <string name="account_recovery_cancel_now">Poništi ponovno postavljanje lozinke</string>
  <string name="account_recovery_cancel_back">Natrag</string>
  <!-- Account recovery cancelled -->
  <string name="account_recovery_cancelled_title">Ponovno postavljanje lozinke otkazano</string>
  <string name="account_recovery_cancelled_subtitle">Ako niste zatražili zahtjev za promjenu lozinke, vaš račun može biti u opasnosti.\n\nZaštitite vaš račun promjenom lozinke.</string>
  <!-- Password change period -->
  <string name="account_recovery_password_started_title">Ponovno postavljanje lozinke dostupno</string>
  <string name="account_recovery_password_started_subtitle">Da biste ponovno postavili vašu lozinku vratite se na uređaj i pretraživač na kojemu ste zatražili ponovno postavljanje.\n\nOvo ćete morati napraviti prije %s.\n\nAko niste zatražili ponovno postavljanje lozinke, otkažite postavljanje sada.</string>
  <!-- Recovery window ended -->
  <string name="account_recovery_window_ended_title">Ponovno postavljanje lozinke isteklo</string>
  <string name="account_recovery_window_ended_subtitle">Morat ćete zatražiti novo postavljanje lozinke za %s.</string>
  <!-- Other -->
  <string name="account_recovery_cancel">Poništi zahtjev</string>
  <string name="account_recovery_dismiss">Razumijem</string>
</resources>
