<resources>
  <!-- Grace period - info -->
  <string name="account_recovery_grace_period_info_title">Wachtwoord reset aangevraagd</string>
  <plurals name="account_recovery_grace_period_info_subtitle">
    <item quantity="one">Een wachtwoordherstel is aangevraagd voor <b>%1$s</b>. U kunt uw wachtwoord wijzigen over %2$d uur.\n\nAls u dit niet heeft aangevraagd, annuleer het nu.</item>
    <item quantity="other">Een wachtwoordherstel is aangevraagd voor <b>%1$s</b>. U kunt uw wachtwoord wijzigen over %2$d uur.\n\nAls u dit niet heeft aangevraagd, annuleer het nu.</item>
  </plurals>
  <!-- Grace period - cancellation form -->
  <string name="account_recovery_cancel_title">Wachtwoordherstel annuleren?</string>
  <string name="account_recovery_cancel_subtitle">Voer uw wachtwoord om het te annuleren.</string>
  <string name="account_recovery_cancel_password_label">Huidig wachtwoord</string>
  <string name="account_recovery_cancel_password_placeholder">Wachtwoord</string>
  <string name="account_recovery_cancel_now">Wachtwoordherstel annuleren</string>
  <string name="account_recovery_cancel_back">Terug</string>
  <!-- Account recovery cancelled -->
  <string name="account_recovery_cancelled_title">Wachtwoordherstel geannuleerd</string>
  <string name="account_recovery_cancelled_subtitle">Als u nooit heeft verzocht om uw wachtwoord te veranderen, kan uw account in gevaar zijn.\n\nBeveilig uw account door uw wachtwoord te wijzigen.</string>
  <!-- Password change period -->
  <string name="account_recovery_password_started_title">Wachtwoordherstel beschikbaar</string>
  <string name="account_recovery_password_started_subtitle">Om uw wachtwoord te herstellen gaat u terug naar uw apparaat en browser die u heeft gebruikt om een wachtwoordherstel te verzoeken.\n\nU moet dit doen voor %s.\n\nAls u geen verzoek heeft gedaan om uw wachtwoord te herstellen, annuleer het herstel nu.</string>
  <!-- Recovery window ended -->
  <string name="account_recovery_window_ended_title">Wachtwoordherstel verlopen</string>
  <string name="account_recovery_window_ended_subtitle">U moet een nieuw wachtwoord aanvragen voor %s.</string>
  <!-- Other -->
  <string name="account_recovery_cancel">Aanvraag annuleren</string>
  <string name="account_recovery_dismiss">Begrepen</string>
</resources>
