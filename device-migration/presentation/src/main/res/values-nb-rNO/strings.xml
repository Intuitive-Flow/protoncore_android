  <string name="emd_code_not_recognized">Koden ble ikke gjenkjent. Prøv igjen.</string>
  <string name="edm_code_verifying">Verifiserer …</string>
  <string name="edm_missing_camera_permission_headline">Tillat kameratilgang</string>
  <string name="edm_missing_camera_permission_body">For å logge på med QR-kode må du gi %1$s tilgang til kameraet ditt. Du kan gjøre dette i enhetsinnstillingene.</string>
  <string name="edm_missing_camera_permission_footer">Du har alltid kontrollen: du kan endre dette når som helst i enhetsinnstillingene.</string>
  <string name="edm_missing_camera_permission_settings">Innstillinger</string>
  <string name="intro_origin_sign_in_title">Logg på en annen enhet</string>
  <string name="intro_origin_sign_in_subtitle">Logg på med QR-koden din</string>
  <string name="intro_origin_sign_in_hint_1">Bruk denne enheten til å skanne QR-koden som vises på enheten du vil logge på. Begge enhetene vil forbli aktive.</string>
  <string name="intro_origin_sign_in_hint_2">For å få tilgang til QR-koden, trykker du på <b>Logg på med QR-kode</b>-knappen på påloggingsskjermen.</string>
  <string name="intro_origin_sign_in_tip_1">Vær forsiktig med meldinger som inneholder en QR-kode. Skann aldri en QR-kode fra et nettsted eller en person du ikke stoler på.</string>
  <string name="intro_origin_sign_in_tip_2">Proton vil aldri be deg om å skanne en QR-kode som ikke kommer fra domenet proton.me.</string>
  <string name="intro_origin_sign_in_begin">Skann QR-kode</string>
  <string name="intro_origin_biometrics_title">Bekreft at det er deg</string>
  <string name="intro_origin_biometrics_subtitle">Lås opp for å logge på med denne appen.</string>
  <string name="manual_code_input_title">Skriv inn koden manuelt</string>
  <string name="manual_code_input_description">For å få tilgang til koden for enheten du vil logge på, trykker du på <b>Logg på med QR-kode</b>-knappen i hjelpmenyen på påloggingsskjermen, og trykker deretter på <b>Skriv inn nøkkel manuelt</b>.</string>
  <string name="manual_code_input_code_label">Kode</string>
  <string name="manual_code_input_clear_code">Tøm</string>
  <string name="manual_code_input_submit">Fortsett</string>
  <string name="origin_success_signed_in">Du er logget på</string>
  <string name="origin_success_message">Du er logget på %1$s på den andre enheten.</string>
  <string name="origin_success_sign_out_hint">Hvis du vil, kan du logge av denne enheten.</string>
  <string name="origin_success_sign_out">Logg av</string>
  <string name="origin_success_ack">Skjønner</string>
  <string name="qr_code_scan_hint">Plasser QR-koden innenfor firkanten</string>
  <string name="qr_code_scan_enter_manually">Skriv inn koden manuelt</string>
  <string name="target_sign_in_title">Logg på med en annen enhet</string>
  <string name="target_sign_in_scan_code">Skann denne koden med telefonkameraet for å logge på umiddelbart.</string>
  <string name="target_sign_in_instructions">1. Åpne Proton-appen på telefonen din\n2. Trykk på <b>Innstillinger</b>, og trykk deretter på <b>Logg på en annen enhet</b>\n3. Trykk på <b>Skann QR-kode</b></string>
  <string name="target_sign_in_error_title">Noe gikk galt</string>
