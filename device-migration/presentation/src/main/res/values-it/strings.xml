  <string name="emd_code_not_recognized">Codice non valido</string>
  <string name="edm_code_verifying">Verifica in corso</string>
  <string name="edm_missing_camera_permission_headline">Autorizzazione necessaria</string>
  <string name="edm_missing_camera_permission_body">Consenti a %1$s l\'accesso alla fotocamera per scansionare il codice QR.</string>
  <string name="edm_missing_camera_permission_footer">Puoi cambiare questa opzione in qualsiasi momento nelle impostazioni del dispositivo.</string>
  <string name="edm_missing_camera_permission_settings">Consenti</string>
  <string name="intro_origin_sign_in_title">Accedi con un altro dispositivo</string>
  <string name="intro_origin_sign_in_subtitle">Come accedere con un codice QR</string>
  <string name="intro_origin_sign_in_hint_1">1. Apri qualsiasi app Proton nel dispositivo con cui vuoi effettuare l\'accesso</string>
  <string name="intro_origin_sign_in_hint_2">2. Nella pagina di accesso, seleziona <b>Password dimenticata</b> o <b>Problemi di accesso</b></string>
  <string name="intro_origin_sign_in_hint_3">3. Seleziona <b>Accedi con un codice QR</b>. Apparirà un codice QR.</string>
  <string name="intro_origin_sign_in_hint_4">4. Con questo dispositivo, tocca <b>Scansiona il codice QR</b> e scansiona il codice.</string>
  <string name="intro_origin_sign_in_tips">Consigli di sicurezza</string>
  <string name="intro_origin_sign_in_tip_1">Non scansionare mai un codice QR proveniente da un messaggio o un sito web non affidabile.</string>
  <string name="intro_origin_sign_in_tip_2">Proton non ti contatterà mai chiedendo di accedere con un codice QR.</string>
  <string name="intro_origin_sign_in_tip_3">Proton non chiederà mai di scansionare un codice QR che non proviene dal dominio <b>https://proton.me</b> o da un\'app Proton ufficiale.</string>
  <string name="intro_origin_sign_in_begin">Scansiona un codice QR</string>
  <string name="intro_origin_biometrics_title">Sblocca il dispositivo per continuare</string>
  <string name="manual_code_input_title">Inserisci manualmente il codice</string>
  <string name="manual_code_input_description">Tocca <b>Accedi con un codice QR</b> nel menu di aiuto della pagina di accesso, poi tocca <b>Inserisci manualmente il codice</b> per visualizzare il codice.</string>
  <string name="manual_code_input_code_label">Codice</string>
  <string name="manual_code_input_clear_code">Cancella</string>
  <string name="manual_code_input_submit">Continua</string>
  <string name="origin_success_signed_in">Accesso effettuato</string>
  <string name="origin_success_message">Accesso effettuato a %1$s con l\'altro dispositivo</string>
  <string name="origin_success_sign_out_hint">Puoi rimuovere l\'accesso quando vuoi.</string>
  <string name="origin_success_sign_out">Esci</string>
  <string name="origin_success_ack">OK</string>
  <string name="qr_code_scan_hint">Posiziona il codice QR all\'interno del quadrato.</string>
  <string name="qr_code_scan_enter_manually">Inserisci manualmente il codice</string>
  <string name="target_sign_in_title">Accedi con un codice QR</string>
  <string name="target_sign_in_scan_code">Come accedere con un altro dispositivo</string>
  <string name="target_sign_in_instruction_1">1. Prendi un altro dispositivo con cui hai già effettuato l\'accesso all\'account Proton.</string>
  <string name="target_sign_in_instruction_2">2. Con quel dispositivo, apri qualsiasi app Proton e seleziona <b>Impostazioni</b></string>
  <string name="target_sign_in_instruction_3">3. Seleziona <b>Accedi con un altro dispositivo → Scansiona un codice QR</b></string>
  <string name="target_sign_in_instruction_4">4. Scansiona il codice per accedere</string>
  <string name="target_sign_in_error_title">Accesso non riuscito</string>
  <string name="target_sign_in_retryable_error">Generazione del codice QR non riuscita</string>
  <string name="target_sign_in_passphrase_error">Accesso non riuscito</string>
  <string name="target_sign_in_error_new_qr">Genera un codice QR</string>
  <string name="target_sign_in_error_back_to_signin">Torna alla pagina di accesso</string>
  <string name="target_sign_in_qr_code_failure">Caricamento del codice QR non riuscito</string>
  <string name="target_sign_in_no_connection">Nessuna connessione a Internet</string>
